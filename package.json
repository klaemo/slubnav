{
  "name": "slub-nav",
  "version": "1.0.0",
  "description": "Indoornavigation für die SLUB Dresden",
  "main": "index.js",
  "scripts": {
    "deploy": "gulp deploy",
    "lint": "jshint . && jscs .",
    "ls": "npm ls",
    "test": "mocha -R spec",
    "start": "npm run watch-css & npm run watch-js & drudge -t -d public",
    "build": "gulp build",
    "deploy": "npm ls && npm run lint && gulp deploy",
    "watch-js": "watchify index.js -d -o public/app.built.js",
    "watch-css": "nodemon --watch styles -e css --exec 'myth styles/index.css public/app.built.css'"
  },
  "browserify": {
    "transform": [
      "jadeify"
    ]
  },
  "author": "Clemens Stolle <klaemo@fastmail.fm>",
  "repository": {
    "type": "git",
    "url": "git://github.com/klaemo/slub-nav"
  },
  "contributors": [
    {
      "name": "Fabian Gündel"
    },
    {
      "name": "Oliver Lenz"
    },
    {
      "name": "Leo Käßner"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "browserify": "^7.1.0",
    "del": "^1.1.1",
    "drudge": "^2.0.0",
    "gulp": "^3.8.10",
    "gulp-awspublish": "0.0.23",
    "gulp-myth": "^1.0.2",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^2.0.1",
    "jade": "^1.7.0",
    "jadeify": "^3.1.0",
    "jscs": "^1.8.0",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1",
    "myth": "^1.2.1",
    "nodemon": "^1.2.1",
    "pre-commit": "0.0.9",
    "uglifyify": "^2.6.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1"
  },
  "dependencies": {
    "ampersand-dom": "^1.2.7",
    "ampersand-router": "^1.0.6",
    "ampersand-state": "^4.4.4",
    "ampersand-view": "^7.2.0",
    "ampersand-view-switcher": "^1.1.2",
    "domready": "^1.0.7",
    "fastclick": "^1.0.3",
    "hammerjs": "^2.0.4",
    "normalize.css": "^3.0.2",
    "popover": "^1.0.3",
    "raf-component": "^1.1.2",
    "transform-style": "^0.1.0"
  },
  "pre-commit": [
    "lint",
    "ls"
  ]
}
